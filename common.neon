		cmsMainDefault:
			group: cms-main
			position: 0
			rights:
				- cms__menu
			items:
				dashboard:
					title: cms.menu.dashboard
					icon: 'fas fa-tachometer-alt'
					route: ':Admin:Homepage:default'
					position: 0
					#badgeHandler: cmsDashboardBadgeHandler
					rights:
						- cms
