{define mainContent}
	<!-- Content Wrapper. Contains page content -->
	<div class="content-wrapper">
		<!-- Content Header (Page header) -->
		<section class="content-header">
			<div class="container-fluid">
				<div class="row mb-2">
					<div class="col-sm-6">
						{include pageTitle}
					</div>
					{ifset breadcrumb}
						<div class="col-sm-6">
							<ol class="breadcrumb float-sm-right">
								{include breadcrumb}
							</ol>
						</div>
					{/ifset}
				</div>
			</div><!-- /.container-fluid -->
		</section>

		<!-- Main content -->
		<section class="content">
			{include content}
		</section>
		<!-- /.content -->
	</div>
	<!-- /.content-wrapper -->
{/define}

