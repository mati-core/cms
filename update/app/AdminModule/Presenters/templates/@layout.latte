	<!-- Font Awesome -->
	<link rel="stylesheet" href="{$basePath}/assets/cms/plugin/fontawesome/css/all.min.css">
	<script href="{$basePath}/assets/cms/plugin/fontawesome/js/all.min.js"></script>

	<!-- Ionicons -->
	<link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">

	<!-- icheck bootstrap -->
	<link rel="stylesheet"
		  href="/assets/cms/plugin/adminlte/plugins/icheck-bootstrap/icheck-bootstrap.min.css">

	<!-- Theme style -->
	<link rel="stylesheet" href="{$basePath}/assets/cms/plugin/adminlte/dist/css/adminlte.min.css">
	<link rel="stylesheet" href="{$basePath}/assets/cms/css/main.css">

	<!-- Google Font: Source Sans Pro -->
